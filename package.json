{
  "name": "vue-image-slider",
  "version": "1.0.1",
  "description": "Simple image slider component",
  "main": "dist/vue-image-slider.ssr.js",
  "module": "dist/vue-image-slider.esm.js",
  "unpkg": "dist/vue-image-slider.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/haykoyaghubyan/vue-image-slider"
  },
  "keywords": [
    "Vue",
    "carousel",
    "image",
    "gallery",
    "slider",
    "responsive"
  ],
  "author": "Hayk Yaghubyan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haykoyaghubyan/vue-image-slider/issues"
  },
  "homepage": "https://github.com/haykoyaghubyan/vue-image-slider",
  "dependencies": {},
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "rollup": "^1.12.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
